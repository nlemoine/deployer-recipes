{
  "name": "n5s/deployer-recipes",
  "description": "",
  "license": "MIT",
  "require": {
    "php": "^7.3 || ^8.0",
    "deployer/deployer": "^7.3.2"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.41",
    "symplify/easy-coding-standard": "^12"
  },
  "autoload": {
    "files": [
      "src/functions.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true
    },
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "cs": "ecs check",
    "cs:fix": "ecs check --fix"
  }
}
